{
    "version": 2,
    "builds": [
      {
        "src": "dist/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/server.js"
      }
    ],
    "headers": [
      {
        "source": "*", 
        "key": "Access-Control-Allow-Origin",
        "value": "*" 
      }
    ]
  }